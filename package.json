{
  "name": "@d3p1/torch-effect",
  "version": "0.0.0",
  "description": "A torch effect to improve my JS skills",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "lint": "npx eslint src",
    "lint:fix": "npm run lint -- --fix",
